{
  "framework": "other",
  "buildCommand": "./gradlew :composeApp:composeCompatibilityBrowserDistribution",
  "outputDirectory": "composeApp/build/dist/composeWebCompatibility/productionExecutable",
  "rewrites": [
    {
      "source": "/projects/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://manpreet.fyi https://www.manpreet.fyi"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|mjs|css|wasm|map|ttf|xml)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

